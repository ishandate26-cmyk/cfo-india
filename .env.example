# Database (Supabase) - Get from: Supabase Dashboard > Settings > API
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Analytics (PostHog) - Get from: PostHog > Project Settings
NEXT_PUBLIC_POSTHOG_KEY=phc_your-key
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Error Tracking (Sentry) - Get from: Sentry > Project > Settings > Client Keys
NEXT_PUBLIC_SENTRY_DSN=https://your-key@sentry.io/your-project

# Claude API (optional - leave empty for mock responses)
ANTHROPIC_API_KEY=

# Local development fallback
DATABASE_URL="file:./dev.db"
